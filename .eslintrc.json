{
  "extends": [
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:no-unsanitized/DOM",
    "prettier"
  ],
  "rules": {
    "no-console": 2
  },
  "plugins": ["import"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true,
    "webextensions": true
  }
}
